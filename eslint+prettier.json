{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "eslint",
        "prettier",
        "vue-eslint-parser",
        "@babel/eslint-parser"
      ],
      "matchPackagePatterns": [
        "^@typescript-eslint/.*",
        "^@angular-eslint/.*",
        "^eslint-plugin-.*",
        "^eslint-config-.*"
      ],
      "groupName": "eslint+prettier dependencies"
    }
  ]
}
